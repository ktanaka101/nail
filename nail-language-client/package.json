{
  "name": "nail-language-client",
  "description": "The client of a language server for VSCode",
  "author": "ktanaka101",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "main": "./out/extension",
  "contributes": {
    "languages": [
      {
        "id": "nail",
        "extensions": [
          ".nail"
        ],
        "aliases": [
          "Nail"
        ]
      }
    ]
  },
  "dependencies": {
    "vscode-languageclient": "^8.0.2"
  },
  "devDependencies": {
    "@types/node": "^20.11.26",
    "@types/vscode": "~1.75.1",
    "@vscode/test-electron": "^2.2.0",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "8.19.2"
  }
}