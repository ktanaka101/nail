{
  "name": "nail-language-client",
  "description": "The client of a language server for VSCode",
  "author": "ktanaka101",
  "license": "MIT",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.87.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "nail",
        "extensions": [
          ".nail"
        ],
        "aliases": [
          "Nail"
        ]
      }
    ]
  },
  "activationEvents": [
    "onLanguage:nail"
  ],
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {
    "@types/vscode": "^1.87.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "volta": {
    "node": "18.12.1",
    "npm": "8.19.2"
  }
}
